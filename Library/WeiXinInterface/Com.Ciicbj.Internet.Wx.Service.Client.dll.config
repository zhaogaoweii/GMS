<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <appSettings>
    <!--是否允许调用微信WCF服务,1允许调用,0不允许调用-->
    <add key="Wx.Wcf.IsAllowCallWxWcfService" value="1"/>
    <!--是否记录调用微信WCF服务成功的日志,1记录,0不记录；调用失败的一定记录-->
    <add key="Wx.Wcf.IsCallSuccessLog" value="1" />
    <!-- 微信系统桥连接WCF服务地址配置 测试地址192.168.40.188-->
    <add key="Wx.Wcf.WWxBizPointService" value="http://localhost:50002/WWxBizPointService/mex/1" />
    <add key="Wx.Wcf.WWxHealthService" value="http://localhost:50002/WWxHealthService/mex/1" />
    <add key="Wx.Wcf.WWxCustomService" value="http://localhost:50002/WWxCustomService/mex/1" />
    <add key="Wx.Wcf.WWxSheBaoService" value="http://localhost:50002/WWxSheBaoService/mex/1" />
    <add key="Wx.Wcf.WWxGjjService" value="http://localhost:50002/WWxGjjService/mex/1" />
    <add key="Wx.Wcf.WWxHrService" value="http://localhost:50002/WWxHrService/mex/1" />
    <add key="Wx.Wcf.WWxHrArchivesService" value="http://localhost:50002/WWxHrArchivesService/mex/1" />
    
  </appSettings>

  <connectionStrings>
    <add name="OMSDB" connectionString="User ID=oms_v3_dev;Password=oms_v3_dev;Data Source=ora2813;Enlist=false;"/>
  </connectionStrings>

</configuration>